#!/bin/bash

if [ "yes" == "$FAIL" ]; then 
    >&2 echo "This is just a failing script."
    exit 1
fi

echo "{
  \"accessToken\": \"...\",
  \"expiresOn\": \"$(date -v+${EXPIRE:=10S} +'%F %T')\",
  \"subscription\": \"aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee\",
  \"tenant\": \"aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee\",
  \"tokenType\": \"Bearer\"
}"